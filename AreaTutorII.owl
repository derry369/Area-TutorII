<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.org/shapes#"
     xml:base="http://www.example.org/shapes"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">

    <owl:Ontology rdf:about="http://www.example.org/shapes"/>

    <!-- ===================== CLASSES ===================== -->
    <owl:Class rdf:about="#Shape"/>
    <owl:Class rdf:about="#Square"><rdfs:subClassOf rdf:resource="#Shape"/></owl:Class>
    <owl:Class rdf:about="#Rectangle"><rdfs:subClassOf rdf:resource="#Shape"/></owl:Class>
    <owl:Class rdf:about="#Triangle"><rdfs:subClassOf rdf:resource="#Shape"/></owl:Class>
    <owl:Class rdf:about="#Circle"><rdfs:subClassOf rdf:resource="#Shape"/></owl:Class>
    <owl:Class rdf:about="#Parallelogram"><rdfs:subClassOf rdf:resource="#Shape"/></owl:Class>
    <owl:Class rdf:about="#Trapezium"><rdfs:subClassOf rdf:resource="#Shape"/></owl:Class>

    <owl:Class rdf:about="#Formula"/>
    <owl:Class rdf:about="#AreaFormula"><rdfs:subClassOf rdf:resource="#Formula"/></owl:Class>

    <owl:Class rdf:about="#ErrorType"/>
    <owl:Class rdf:about="#Hint"/>

    <!-- ===================== OBJECT PROPERTIES ===================== -->
    <owl:ObjectProperty rdf:about="#hasFormula">
        <rdfs:domain rdf:resource="#Shape"/>
        <rdfs:range rdf:resource="#Formula"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasError">
        <rdfs:domain rdf:resource="#Formula"/>
        <rdfs:range rdf:resource="#ErrorType"/>
    </owl:ObjectProperty>
    <owl:ObjectProperty rdf:about="#hasHint">
        <rdfs:domain rdf:resource="#Formula"/>
        <rdfs:range rdf:resource="#Hint"/>
    </owl:ObjectProperty>

    <!-- ===================== DATA PROPERTIES ===================== -->
    <owl:DatatypeProperty rdf:about="#formulaExpression">
        <rdfs:domain rdf:resource="#Formula"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="#tolerance">
        <rdfs:domain rdf:resource="#Formula"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="#description">
        <rdfs:domain rdf:resource="#Hint"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>
    <owl:DatatypeProperty rdf:about="#errorDescription">
        <rdfs:domain rdf:resource="#ErrorType"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:DatatypeProperty>

    <!-- ===================== INDIVIDUALS ===================== -->

    <!-- ========== SQUARE ========= -->
    <owl:NamedIndividual rdf:about="#SquareIndividual">
        <rdf:type rdf:resource="#Square"/>
        <hasFormula rdf:resource="#SquareAreaFormula"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#SquareAreaFormula">
        <rdf:type rdf:resource="#AreaFormula"/>
        <formulaExpression rdf:datatype="http://www.w3.org/2001/XMLSchema#string">s*s</formulaExpression>
        <tolerance rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.01</tolerance>
        <hasHint rdf:resource="#SquareHint1"/>
        <hasHint rdf:resource="#SquareHint2"/>
        <hasError rdf:resource="#SquareErrorMissSide"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#SquareHint1">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area = side squared (s × s)</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#SquareHint2">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remember: multiply the side by itself: s × s</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#SquareErrorMissSide">
        <rdf:type rdf:resource="#ErrorType"/>
        <errorDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Did you forget to square the side?</errorDescription>
    </owl:NamedIndividual>

    <!-- ========== RECTANGLE ========= -->
    <owl:NamedIndividual rdf:about="#RectangleIndividual">
        <rdf:type rdf:resource="#Rectangle"/>
        <hasFormula rdf:resource="#RectangleAreaFormula"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#RectangleAreaFormula">
        <rdf:type rdf:resource="#AreaFormula"/>
        <formulaExpression rdf:datatype="http://www.w3.org/2001/XMLSchema#string">l*w</formulaExpression>
        <tolerance rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.01</tolerance>
        <hasHint rdf:resource="#RectangleHint1"/>
        <hasHint rdf:resource="#RectangleHint2"/>
        <hasError rdf:resource="#RectangleErrorSwappedSides"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#RectangleHint1">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area = length × width</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#RectangleHint2">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Multiply length and width in any order</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#RectangleErrorSwappedSides">
        <rdf:type rdf:resource="#ErrorType"/>
        <errorDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Did you confuse length and width?</errorDescription>
    </owl:NamedIndividual>

    <!-- ========== TRIANGLE ========= -->
    <owl:NamedIndividual rdf:about="#TriangleIndividual">
        <rdf:type rdf:resource="#Triangle"/>
        <hasFormula rdf:resource="#TriangleAreaFormula"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TriangleAreaFormula">
        <rdf:type rdf:resource="#AreaFormula"/>
        <formulaExpression rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.5*b*h</formulaExpression>
        <tolerance rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.01</tolerance>
        <hasHint rdf:resource="#TriangleHint1"/>
        <hasHint rdf:resource="#TriangleHint2"/>
        <hasError rdf:resource="#TriangleErrorMissHalf"/>
        <hasError rdf:resource="#TriangleErrorUsedBaseOnly"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TriangleHint1">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area = ½ × base × height</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TriangleHint2">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remember to include the ½ factor when multiplying base and height</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TriangleErrorMissHalf">
        <rdf:type rdf:resource="#ErrorType"/>
        <errorDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Forgot the ½ factor</errorDescription>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TriangleErrorUsedBaseOnly">
        <rdf:type rdf:resource="#ErrorType"/>
        <errorDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Used only the base, forgot to multiply by height</errorDescription>
    </owl:NamedIndividual>

    <!-- ========== CIRCLE ========= -->
    <owl:NamedIndividual rdf:about="#CircleIndividual">
        <rdf:type rdf:resource="#Circle"/>
        <hasFormula rdf:resource="#CircleAreaFormula"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#CircleAreaFormula">
        <rdf:type rdf:resource="#AreaFormula"/>
        <formulaExpression rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pi*r^2</formulaExpression>
        <tolerance rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.01</tolerance>
        <hasHint rdf:resource="#CircleHint1"/>
        <hasHint rdf:resource="#CircleHint2"/>
        <hasError rdf:resource="#CircleErrorUsedDiameter"/>
        <hasError rdf:resource="#CircleErrorCircumference"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#CircleHint1">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use π and square the radius</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#CircleHint2">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Formula: π × r² → plug in the radius value</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#CircleErrorUsedDiameter">
        <rdf:type rdf:resource="#ErrorType"/>
        <errorDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You used the diameter instead of the radius</errorDescription>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#CircleErrorCircumference">
        <rdf:type rdf:resource="#ErrorType"/>
        <errorDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">That's the circumference, not the area. Use π r²</errorDescription>
    </owl:NamedIndividual>

    <!-- ========== PARALLELOGRAM ========= -->
    <owl:NamedIndividual rdf:about="#ParallelogramIndividual">
        <rdf:type rdf:resource="#Parallelogram"/>
        <hasFormula rdf:resource="#ParallelogramAreaFormula"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#ParallelogramAreaFormula">
        <rdf:type rdf:resource="#AreaFormula"/>
        <formulaExpression rdf:datatype="http://www.w3.org/2001/XMLSchema#string">base*height</formulaExpression>
        <tolerance rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.01</tolerance>
        <hasHint rdf:resource="#ParallelogramHint1"/>
        <hasHint rdf:resource="#ParallelogramHint2"/>
        <hasError rdf:resource="#ParallelogramErrorUsedSide"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#ParallelogramHint1">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Area = base × height</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#ParallelogramHint2">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Do not use slanted sides, only perpendicular height</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#ParallelogramErrorUsedSide">
        <rdf:type rdf:resource="#ErrorType"/>
        <errorDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Used slanted side instead of height</errorDescription>
    </owl:NamedIndividual>

    <!-- ========== TRAPEZIUM ========= -->
    <owl:NamedIndividual rdf:about="#TrapeziumIndividual">
        <rdf:type rdf:resource="#Trapezium"/>
        <hasFormula rdf:resource="#TrapeziumAreaFormula"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TrapeziumAreaFormula">
        <rdf:type rdf:resource="#AreaFormula"/>
        <formulaExpression rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.5*(a+b)*h</formulaExpression>
        <tolerance rdf:datatype="http://www.w3.org/2001/XMLSchema#float">0.01</tolerance>
        <hasHint rdf:resource="#TrapeziumHint1"/>
        <hasHint rdf:resource="#TrapeziumHint2"/>
        <hasError rdf:resource="#TrapeziumErrorForgotHalf"/>
        <hasError rdf:resource="#TrapeziumErrorSingleSide"/>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TrapeziumHint1">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remember: only the two parallel sides go inside the brackets. Formula starts with ½ × ( ... )</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TrapeziumHint2">
        <rdf:type rdf:resource="#Hint"/>
        <description rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Full formula: ½ × (a + b) × h → plug in a and b, calculate h yourself if not shown</description>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TrapeziumErrorForgotHalf">
        <rdf:type rdf:resource="#ErrorType"/>
        <errorDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Almost! You forgot to multiply by ½</errorDescription>
    </owl:NamedIndividual>
    <owl:NamedIndividual rdf:about="#TrapeziumErrorSingleSide">
        <rdf:type rdf:resource="#ErrorType"/>
        <errorDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You used only one parallel side. Use both: ½ × (a+b) × h</errorDescription>
    </owl:NamedIndividual>

</rdf:RDF>
